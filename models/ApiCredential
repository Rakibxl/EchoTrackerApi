// models/ApiCredential.js

const mongoose = require('mongoose');

const apiCredentialSchema = new mongoose.Schema({
  serviceProvider: {
    type: String,
    required: true
  },
  apiKey: {
    type: String,
    required: true
  },
  password: {
    type: String,
    required: true
  }
});

module.exports = mongoose.model('ApiCredential', apiCredentialSchema);
